{
  "name": "unipdf-studio-sprint-management",
  "version": "1.0.0",
  "description": "Sprint management system for UniPDF Studio project",
  "scripts": {
    "sync-sprints": "node scripts/sync-sprints.js",
    "sprint-status": "node scripts/sprint-status.js",
    "validate-sprints": "node scripts/validate-sprints.js",
    "sprint-help": "cat documentation/SPRINT-MANAGEMENT-README.md",
    "view-changelog": "cat documentation/SPRINT-CHANGELOG.md | tail -100",
    "backup-sprints": "node scripts/backup-sprints.js",
    "full-sync": "npm run validate-sprints && npm run sync-sprints && npm run sprint-status"
  },
  "keywords": [
    "sprint",
    "agile",
    "project-management",
    "scrum"
  ],
  "author": "UniPDF Studio Team",
  "license": "MIT",
  "dependencies": {
    "@excalidraw/excalidraw": "^0.18.0",
    "@tiptap/extension-color": "^3.10.7",
    "@tiptap/extension-highlight": "^3.10.7",
    "@tiptap/extension-text-style": "^3.10.7",
    "@tiptap/react": "^3.10.7",
    "@tiptap/starter-kit": "^3.10.7",
    "js-yaml": "^4.1.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
